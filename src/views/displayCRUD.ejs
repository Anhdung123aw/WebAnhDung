<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Get CRUD</title>
    <style>
      table {
        font-family: arial, sans-serif;
        border-collapse: collapse;
        width: 100%;
      }
      
      td, th {
        border: 1px solid #dddddd;
        text-align: left;
        padding: 8px;
      }
      
      tr:nth-child(even) {
        background-color: #dddddd;
      }
      .btn-edit{
        border: none;
        outline: none;
        height: 25px;
        width: 60px;
        border-radius: 5px;
        background-color: green;
        cursor: pointer;
        display: inline-block;
      }
      .btn-delete{
        border: none;
        outline: none;
        height: 25px;
        width: 60px;
        border-radius: 5px;
        background-color: red;
        cursor: pointer;
        display: inline-block;
      }
      </style>
  </head>
  <body>
<h2>HTML Table</h2>
<table>
  <tr>
    <th>Email</th>
    <th>FirstName</th>
    <th>LastName</th>
    <th>Address</th>
    <th>PhoneNumber</th>
    <th>Acctions</th>
  </tr>
  <% for(var i=0; i < dataTable.length; i++) { %>
    <tr>
      <td><%= dataTable[i].email %></td>
      <td><%= dataTable[i].firstName %></td>
      <td><%= dataTable[i].lastName %></td>
      <td><%= dataTable[i].address %></td>
      <td><%= dataTable[i].phonenumber %></td>
      <td>
        <a href="/edit-crud?id=<%= dataTable[i].id%>" class="btn-edit">Edit</a>
        <a href="/delete-crud?id=<%= dataTable[i].id%>" class="btn-delete">Delete</a>
    </tr>
 <% } %>
  
  
</table>

</body>
</html>


  </body>
</html>
